<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hima.miwok">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".NumbersActivity" android:label="@string/number_name"/>

        <activity android:name=".ColorsActivity" android:label="@string/color_name"/>

        <activity android:name=".FamilyActivity" android:label="@string/family_name"/>

        <activity android:name=".PharesesActivity" android:label="@string/phrases_name"/>

    </application>

</manifest>